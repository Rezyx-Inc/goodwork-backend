        "laravel-mix": "^5.0.7",
        "lodash": "^4.17.21",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.27.0",
        "sass-loader": "^8.0.0",
        "vue": "^2.6.12",
        "vue-loader": "^15.0.0",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "@vue/compiler-sfc": "^3.3.9",
        "body-parser": "^1.20.2",
        "chart.js": "^4.4.2",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.1",
        "laravel-echo": "^1.15.3",
        "mongoose": "^8.2.3",
        "node-cron": "^3.0.3",
        "path": "^0.12.7",
        "pm2": "^5.3.1",
        "socket.io-client": "^2.5.0",
        "stripe": "^15.1.0",
        "update": "^0.7.4",
