{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "aranyasen/laravel-adminer": "^2.0",
        "bensampo/laravel-enum": "^6.11.1",
        "beyondcode/laravel-websockets": "^1.14",
        "bugsnag/bugsnag-laravel": "^2.0",
        "doctrine/cache": "2.2.0",
        "doctrine/dbal": "^2.13.9",
        "ejarnutowski/laravel-api-key": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "graham-campbell/throttle": "^10.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "jenssegers/mongodb": "^3.7",
        "laravel/framework": "^9.0",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.4",
        "maatwebsite/excel": "^3.1",
        "mongodb/laravel-mongodb": "3.9.5",
        "mongodb/mongodb": "^1.20.0",
        "nwidart/laravel-modules": "^8.3",
        "phpmailer/phpmailer": "^6.8",
        "realrashid/sweet-alert": "^7.0",
        "spatie/geocoder": "^3.9",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-medialibrary": "^10.15",
        "spatie/laravel-permission": "^5.10",
        "stripe/stripe-php": "^13.1",
        "twilio/sdk": "^7.9",
        "tymon/jwt-auth": "^1.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
